pipeline {
    agent any
    stages{
    
    stage('Build') {
       steps{
       echo "build building project"
    }
    }

     stage('Test') {
        steps{  
       echo "Test building project"
    }
    }
    
     stage('Deploy') {
        steps{  
       echo "Deploy building project"
    }
    }
    
     stage('Release') {
        steps{  
       echo "Release project test...."
    }
    }
}
}
